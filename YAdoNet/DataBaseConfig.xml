<?xml version="1.0" encoding="utf-8" ?>
<!--数据库配置文件示例。-->
<!--DataBaseConfig为数据库配置文件根节点名称-->
<DataBaseConfig>
  
  <!--
  NodeName为节点名称，用来区分多个数据库配置。
  databaseType为数据库类型，如果没配置默认为MSSQL。
  -->
  <NodeName databaseType="MSSQL">
    <!--根据不同类型数据库设置配置文件。-->
  </NodeName>

  <!--
  SQLServer数据库配置示例。
  -->
  <SQLServer databaseType="MSSQL">
    <!--服务器名称，Crypto标识加密类型，目前支持的有AES和DES-->
    <ServerName Crypto="NO">localhost</ServerName>
    <!--数据库端口号，如果不配置则问默认端口，Crypto标识加密类型，目前支持的有AES和DES-->
    <ServerPort Crypto="NO">1433</ServerPort>
    <!--数据库实例名，如果不配置则使用默认实例名，Crypto标识加密类型，目前支持的有AES和DES-->
    <Example Crypto="NO"></Example>
    <!--数据库名称，Crypto标识加密类型，目前支持的有AES和DES-->
    <DataBaseName Crypto="NO">jsw</DataBaseName>
    <!--登陆用户名，Crypto标识加密类型，目前支持的有AES和DES-->
    <UserID Crypto="NO">sa</UserID>
    <!--登陆密码，Crypto标识加密类型，目前支持的有AES和DES-->
    <UserPassword Crypto="NO">123456</UserPassword>
  </SQLServer>
</DataBaseConfig>